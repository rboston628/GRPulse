       RowBox[{"\[CapitalDelta]logn", "[", 
        RowBox[{"t", ",", "r"}], "]"}], 
       RowBox[{"ylm", "[", 
        RowBox[{"\[Theta]", ",", "\[Phi]"}], "]"}]}], " ", "-", " ", 
      RowBox[{"displacement", ".", 
       RowBox[{"{", 
        RowBox[{"0", ",", 
         RowBox[{"D", "[", 
          RowBox[{
           RowBox[{"Log", "[", 
            RowBox[{"P", "[", "r", "]"}], "]"}], ",", "r"}], "]"}], ",", 
         RowBox[{"D", "[", 
          RowBox[{
           RowBox[{"Log", "[", 
            RowBox[{"P", "[", "r", "]"}], "]"}], ",", "\[Theta]"}], "]"}], 
         ",", 
         RowBox[{"D", "[", 
          RowBox[{
           RowBox[{"Log", "[", 
            RowBox[{"P", "[", "r", "]"}], "]"}], ",", "\[Phi]"}], "]"}]}], 
        "}"}]}]}], ",", 
     RowBox[{"ylm", "[", 
      RowBox[{"\[Theta]", ",", "\[Phi]"}], "]"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
