Cell[TextData[{
 "Now find \[Delta]log\[Rho].  \nRecall that ",
 Cell[BoxData[
  FormBox[
   RowBox[{"(", 
    RowBox[{"1", "+", 
     FractionBox["P", "\[Rho]"]}], ")"}], TraditionalForm]],ExpressionUUID->
  "e80ac9de-0c64-4f10-950d-bcba328829d3"],
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox["\[CapitalDelta]logP", 
     RowBox[{"\[CapitalDelta]log\[Rho]", " "}]], "=", 
    RowBox[{
     RowBox[{
      FractionBox[
       RowBox[{"\[Rho]", "+", "P"}], "P"], 
      SubscriptBox[
       RowBox[{"(", 
        FractionBox[
         RowBox[{"\[PartialD]", "P"}], 
         RowBox[{"\[PartialD]", "\[Rho]"}]], ")"}], "ad"]}], "=", 
     SubscriptBox["\[CapitalGamma]", "1"]}]}], TraditionalForm]],
  ExpressionUUID->"687e266f-db7d-4dc9-96f2-0c75f42c0e26"],
 "so that \[CapitalDelta]log\[Rho] = ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox[
     RowBox[{"(", 
      RowBox[{"1", "+", 
       RowBox[{"P", "/", "\[Rho]"}]}], ")"}], 
     SubscriptBox["\[CapitalGamma]", "1"]], "\[CapitalDelta]logP"}], 
   TraditionalForm]],ExpressionUUID->"60fa8b30-d617-4d58-837d-33b683cc1c2e"],
 ", and\n\[Delta]log\[Rho] = \[CapitalDelta]log\[Rho] - ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     OverscriptBox["\[Xi]", "\[RightVector]"], "*", 
     RowBox[{"\[Del]", "log\[Rho]"}]}], " ", "=", " ", 
    RowBox[{
     RowBox[{
      FractionBox[
       RowBox[{"(", 
        RowBox[{"1", "+", 
         RowBox[{"P", "/", "\[Rho]"}]}], ")"}], 
       SubscriptBox["\[CapitalGamma]", "1"]], "\[CapitalDelta]logP"}], " ", 
     "-", " ", 
     RowBox[{
      OverscriptBox["\[Xi]", "\[RightVector]"], "*", 
      RowBox[{"\[Del]", "log"}], " ", "\[Rho]"}]}]}], TraditionalForm]],
  ExpressionUUID->"145ad5d6-e326-4c4e-a8dc-d8703c1d3c0f"],
 " = ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     FractionBox[
      RowBox[{"(", 
       RowBox[{"1", "+", 
        RowBox[{"P", "/", "\[Rho]"}]}], ")"}], 
      SubscriptBox["\[CapitalGamma]", "1"]], 
     RowBox[{"(", 
      RowBox[{"\[Delta]logP", " ", "+", " ", 
       RowBox[{
        OverscriptBox["\[Xi]", "\[RightVector]"], "*", 
        RowBox[{"\[Del]", "logP"}]}]}], ")"}]}], " ", "-", " ", 
    RowBox[{
     OverscriptBox["\[Xi]", "\[RightVector]"], "*", 
     RowBox[{"\[Del]", "log\[Rho]"}]}]}], TraditionalForm]],ExpressionUUID->
  "41542d29-e605-4be1-9811-dfa877630fbd"]
}], "Text",
