Cell[BoxData[{
 RowBox[{
  RowBox[{"spatialMetric1", " ", "=", " ", 
   RowBox[{"Normal", "@", 
    RowBox[{"Series", "[", 
     RowBox[{
      RowBox[{"metric1", "[", 
