Cell[TextData[{
 "Now find \[Delta]log\[Rho].  Recall that ",
 Cell[BoxData[
  FormBox[
   RowBox[{"(", 
    RowBox[{"1", "+", 
     FractionBox["P", "\[Rho]"]}], ")"}], TraditionalForm]],ExpressionUUID->
  "04e3562c-f0cd-4de0-b778-0f44ff8ca8db"],
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox["\[Delta]logP", 
     RowBox[{"\[Delta]log\[Rho]", " "}]], "=", 
    RowBox[{
     RowBox[{
      FractionBox[
       RowBox[{"\[Rho]", "+", "P"}], "P"], 
      SubscriptBox[
       RowBox[{"(", 
        FractionBox[
         RowBox[{"\[PartialD]", "P"}], 
         RowBox[{"\[PartialD]", "\[Rho]"}]], ")"}], "ad"]}], "=", 
     SubscriptBox["\[CapitalGamma]", "1"]}]}], TraditionalForm]],
  ExpressionUUID->"7e20261c-861d-40ad-9b63-651cdcdcd959"],
 "so that \[Delta]log\[Rho] = ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox[
     RowBox[{"(", 
      RowBox[{"1", "+", 
       RowBox[{"P", "/", "\[Rho]"}]}], ")"}], 
     SubscriptBox["\[CapitalGamma]", "1"]], "\[Delta]logP"}], 
   TraditionalForm]],ExpressionUUID->"d70831c5-c7fe-4764-a88c-f74549f5a663"],
 ", and\n\[CapitalDelta]log\[Rho] = \[Delta]log\[Rho] - ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     OverscriptBox["\[Xi]", "\[RightVector]"], "*", 
     RowBox[{"\[Del]", "log\[Rho]"}]}], " ", "=", " ", 
    RowBox[{
     RowBox[{
      FractionBox[
