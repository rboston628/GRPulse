     RowBox[{"Sqrt", "[", 
      RowBox[{"-", 
       RowBox[{"Det", "[", "spatialMetric1", "]"}]}], "]"}], "//", 
     "PowerExpand"}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"root3detg0", " ", "=", " ", 
   RowBox[{"SeriesCoefficient", "[", 
    RowBox[{"root3detg1", ",", 
     RowBox[{"{", 
      RowBox[{"\[Epsilon]", ",", "0", ",", "0"}], "}"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"root3detgPert", " ", "=", " ", 
   RowBox[{"SeriesCoefficient", "[", 
    RowBox[{"root3detg1", ",", 
     RowBox[{"{", 
      RowBox[{"\[Epsilon]", ",", "0", ",", "1"}], "}"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"divgXi", " ", "=", " ", 
   RowBox[{"Sum", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"Normal", "@", 
       RowBox[{"Series", "[", 
        RowBox[{
         RowBox[{"Simplify", "[", 
          FractionBox[
           RowBox[{"difR", "[", 
            RowBox[{
             RowBox[{"root3detg1", "*", " ", 
              RowBox[{"displacement", "[", 
               RowBox[{"[", "j", "]"}], "]"}]}], ",", "j"}], "]"}], 
           "root3detg1"], "]"}], ",", 
         RowBox[{"{", 
          RowBox[{"\[Epsilon]", ",", "0", ",", "0"}], "}"}], ",", 
