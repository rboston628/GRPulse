    RowBox[{"xir", "[", "R", "]"}], " ", 
    RowBox[{"\[Rho]", "[", "R", "]"}]}], 
   SuperscriptBox["c", "3"]], "+", 
  FractionBox[
   RowBox[{
    RowBox[{
     RowBox[{"-", "\[ScriptL]"}], " ", 
     RowBox[{"(", 
      RowBox[{"1", "+", "\[ScriptL]"}], ")"}], " ", 
     RowBox[{"\[CapitalDelta]Wr", "[", "R", "]"}]}], "+", 
    RowBox[{"R", " ", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{"\[ScriptL]", " ", 
        RowBox[{"(", 
         RowBox[{"1", "+", "\[ScriptL]"}], ")"}], " ", 
        RowBox[{
         SuperscriptBox["\[CapitalDelta]WH", "\[Prime]",
          MultilineFunction->None], "[", "R", "]"}]}], "-", 
       RowBox[{"\[ImaginaryI]", " ", "R", " ", "\[Omega]", " ", 
        RowBox[{
         SuperscriptBox["\[CapitalDelta]\[CapitalPhi]", "\[Prime]",
          MultilineFunction->None], "[", "R", "]"}]}]}], ")"}]}]}], 
